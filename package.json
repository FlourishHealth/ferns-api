{
  "name": "ferns-api",
  "version": "0.33.12",
  "description": "Styled after the Django & Django REST Framework, a batteries-include framework for building REST APIs with Node/Express/Mongoose.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint \"src/**/*.ts*\"",
    "lintfix": "eslint --fix \"src/**/*.ts*\"",
    "test": "jest -i",
    "updateSnapshot": "jest --updateSnapshot",
    "docs": "typedoc --out docs src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/FlourishHealth/ferns-api.git"
  },
  "keywords": [
    "rest",
    "api",
    "mongoose",
    "express"
  ],
  "author": "Josh Gachnang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/FlourishHealth/ferns/issues"
  },
  "homepage": "https://github.com/FlourishHealth/ferns#readme",
  "dependencies": {
    "@sentry/node": "^7.75.0",
    "@sentry/profiling-node": "^1.2.1",
    "@wesleytodd/openapi": "^0.2.0",
    "axios": "^1.5.1",
    "cors": "^2.8.5",
    "cron": "^3.1.4",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.17.1",
    "generaterr": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose-to-swagger": "^1.4.0",
    "on-finished": "^2.3.0",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^8.0.0",
    "scmp": "^2.1.0",
    "winston": "^3.11.0"
  },
  "peerDependencies": {
    "mongoose": "^7.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/chai": "^4.3.9",
    "@types/chai-as-promised": "^7.1.7",
    "@types/cors": "^2.8.15",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.8",
    "@types/on-finished": "^2.3.3",
    "@types/passport": "^1.0.9",
    "@types/passport-anonymous": "^1.0.4",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.37",
    "@types/sinon": "^10.0.20",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.52.0",
    "eslint-config-ferns": "^0.4.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "jest": "^29.7.0",
    "mongoose": "^7.6.2",
    "prettier": "^3.0.3",
    "sinon": "^17.0.0",
    "supertest": "^6.1.6",
    "ts-jest": "^29.1.1",
    "typedoc": "~0.25.2",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "json5": "2.2.3"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false
  },
  "eslintConfig": {
    "extends": [
      "ferns"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
